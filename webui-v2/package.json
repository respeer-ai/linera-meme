{
  "name": "linera-meme-webui",
  "version": "0.0.1",
  "description": "Trading/Creating creative minable Meme tokens with Linera",
  "productName": "MicroMeme",
  "author": "Zhao KK <kikakkz@hotmail.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,ts,vue,,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build",
    "postinstall": "quasar prepare",
    "graphql:generate": "graphql-codegen --config graphql-codegen.ts",
    "build:wasm": "echo 'With bun, you have to run \"wasm-pack build --out-dir ../dist/wasm --target web wasm\" manually'",
    "prettier:check": "prettier --check \"**/*.{js,json,md,ts,yml}\"",
    "prettier:write": "prettier --write \"**/*.{js,json,md,ts,yml}\""
  },
  "dependencies": {
    "@apollo/client": "^3.9.11",
    "@graphql-codegen/cli": "^6.1.0",
    "@linera/client": "^0.15.7",
    "@linera/metamask": "^0.15.7",
    "@quasar/extras": "^1.16.4",
    "@vue/apollo-composable": "^4.2.2",
    "axios": "^1.2.1",
    "dexie": "^4.2.1",
    "hash-wasm": "^4.12.0",
    "lightweight-charts": "^5.0.9",
    "pinia": "^3.0.1",
    "quasar": "^2.16.0",
    "react": "^19.2.1",
    "vue": "^3.5.22",
    "vue-i18n": "^11.0.0",
    "vue-router": "^4.0.12",
    "vue3-youtube": "^0.1.9",
    "web3": "^4.16.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@quasar/app-vite": "^2.1.0",
    "@types/node": "^20.5.9",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^10.4.0",
    "globals": "^16.4.0",
    "prettier": "^3.3.3",
    "typescript": "^5.9.2",
    "vite-plugin-checker": "^0.11.0",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.7",
    "wasm-pack": "^0.13.1"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
